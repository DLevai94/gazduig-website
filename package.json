{
  "name": "gazduig-website",
  "description": "Gazduig Agency Website",
  "version": "2.0.0",
  "author": "David Levai <david@davidlevai.com>",
  "dependencies": {
    "@sendgrid/mail": "^7.2.3",
    "@tailwindcss/ui": "^0.4.0",
    "dotenv": "^8.2.0",
    "gatsby": "2.24.11",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "2.4.21",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "^2.6.22",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-remark": "^2.8.27",
    "gatsby-transformer-sharp": "^2.5.11",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  },
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "forestry:preview": "gatsby develop -p 8080 -H 0.0.0.0"
  },
  "devDependencies": {
    "autoprefixer": "9.8.5",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.5.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.3",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "2.0.5",
    "tailwindcss": "1.5.2"
  }
}
